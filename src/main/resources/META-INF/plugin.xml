<idea-plugin>
  <id>com.jenkin.intellij.plugin.setterfieldhelper</id>
  <name>SetterFieldHelper</name>
  <vendor email="303749348@qq.com">jenkinXu</vendor>

  <description><![CDATA[
    <p> Assign value to objects based on input parameters.Support `lombok.Builder` and multi-parameter</p>
    <p> 自动根据入参或者this生成返回参数类型代码</p>
    <p> 创意源自于generateAllSetMethod，快捷键一致</p>
    ]]></description>

  <idea-version since-build="181.00"/>

  <depends>com.intellij.modules.lang</depends>
  <depends>com.intellij.modules.platform</depends>
  <depends>com.intellij.modules.java</depends>


  <actions>
    <!-- Add your actions here -->
      <action id="com.jenkin.intellij.plugin.actions.SetFieldsAction"
              class="com.jenkin.intellij.plugin.actions.SetFieldsAction" text="Set Object Fields"
              description="Set object fields value by params">
          <add-to-group group-id="EditMenu" anchor="last"/>
          <keyboard-shortcut keymap="$default" first-keystroke="shift alt K"/>
      </action>
  </actions>
</idea-plugin>
